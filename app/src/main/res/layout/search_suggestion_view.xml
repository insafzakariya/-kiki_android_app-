<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/searchSugLayout">

    <RelativeLayout
        android:id="@+id/search_sug_list_relative"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_marginStart="@dimen/_10sdp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/contstrain_search_sug"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">



            <lk.mobilevisions.kiki.audio.util.KikiAudioMediumTextView
                android:id="@+id/search_sug_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10sdp"
                android:text="Havana"
                android:textColor="@color/colorPrimary"
                android:textSize="@dimen/_15sdp"
                android:singleLine="true"
                app:layout_constraintStart_toEndOf="@+id/playlist_image"
                app:layout_constraintTop_toTopOf="parent"/>



        </androidx.constraintlayout.widget.ConstraintLayout>

    </RelativeLayout>

</RelativeLayout>